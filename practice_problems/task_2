data class Event(
	val title: String,
    val description: String? = null,
    val daypart: Daypart,
    val duration: Int
)
enum class Daypart{
    MORNING, AFTERNOON, EVENING
}

fun main() {
    val Kotlin = Event("Study Kotlin", "commit to studying Kotlin at least 15 minutes per day.", Daypart.EVENING, 15)
    println(Kotlin)
}
